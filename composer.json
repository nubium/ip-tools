{
    "name": "nubium/ip-tools",
    "description": "Provides tools for IP address",
    "keywords": [
        "ip",
        "geoip",
        "iplist"
    ],
    "license": "MIT",
    "require": {
        "ext-maxminddb": "*",
        "php": ">=7.4",
        "geoip2/geoip2": "^2.12.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.3.0",
        "phpstan/phpstan-phpunit": "^1.0.0",
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan-mockery": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Nubium\\IpTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nubium\\IpTools\\Test\\": "test"
        }
    }
}
