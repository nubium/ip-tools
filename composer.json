{
    "name": "nubium/ip-tools",
    "description": "Provides tools for IP address",
    "keywords": [
        "ip",
        "geoip",
        "iplist"
    ],
    "license": "MIT",
    "require": {
        "ext-geoip": "*",
        "php": ">=7.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.71",
        "phpstan/phpstan-phpunit": "^0.12.17",
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan-mockery": "^0.12.13"
    },
    "autoload": {
        "psr-4": {
            "Nubium\\IpTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nubium\\IpTools\\Test\\": "test"
        }
    }
}
